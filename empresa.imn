node n1 {
    type router
    model PC
    network-config {
	hostname NotebookClient
	!
	interface eth0
	 ip address 192.0.2.160/25
	 ipv6 address 2001:db8:ca5a:2::2/64
	!
    }
    canvas c1
    iconcoords {128.33333333333331 339.66666666666663}
    labelcoords {126.66666666666669 398.6666666666667}
    interface-peer {eth0 n3}
    custom-config {
	custom-config-id service:UserDefined
	custom-command UserDefined
	config {
	dirs=('/var/run/resolvconf', '/etc/resolvconf', )
	files=('client.py', )
	cmdup=('client.py', )
	}
    }
    custom-config {
	custom-config-id service:UserDefined:client.py
	custom-command client.py
	config {
	import socket
	HOST = '200.229.248.10'
	PORT = 5000
	dest = (HOST,PORT)
	tcp = socket.socket(socket.AF_INET ,socket.SOCK_STREAM)
	
	tcp.connect(dest)
	print '"crtl + c" or "crtl + x" to quit'
	msg = raw_input()
	while msg <> '\x18':
		tcp.send(msg)
		msg = raw_input()
	tcp.close()
	
	}
    }
    services {DefaultRoute UserDefined}
}

